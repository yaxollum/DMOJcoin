#include <cstdio>

using namespace std;

int main() 
{
    int n;
    scanf("%d",&n);
    int* dat=new int[n];
    
    for (int i=0;i<n;++i)
    {
        scanf("%d",dat+i);
    }
    
    for (int i=1;i<n;++i)
    {
        dat[i]+=dat[i-1];
    }
    
    int q;
    scanf("%d",&q);
    
    int first,last;
    for (int loop=0;loop<q;++loop)
    {
        scanf("%d%d",&first,&last);
        if (first==0) printf("%d\n",dat[last]);
        else printf("%d\n",dat[last]-dat[first-1]);
    }
    return 0;
}