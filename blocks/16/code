#include <iostream>

using namespace std;

int main()
{
	int tests_n;
	cin>>tests_n;
	
	string f;
	for (int loop=0;loop<tests_n;++loop)
	{
	    cin>>f;
	    
	    if (f=="0") cout << "1";
	    
	    else if (f=="1") cout << "1";

		else if (f=="2") cout << "2";

		else if (f=="3") cout << "6";

		else if (f=="4") cout << "24";

		else if (f=="5") cout << "120";

		else if (f=="6") cout << "720";

		else if (f=="7") cout << "5040";

		else if (f=="8") cout << "40320";

		else if (f=="9") cout << "362880";

		else if (f=="10") cout << "3628800";

		else if (f=="11") cout << "39916800";

		else if (f=="12") cout << "479001600";

		else if (f=="13") cout << "1932053504";

		else if (f=="14") cout << "1278945280";

		else if (f=="15") cout << "2004310016";

		else if (f=="16") cout << "2004189184";

		else if (f=="17") cout << "4006445056";

		else if (f=="18") cout << "3396534272";

		else if (f=="19") cout << "109641728";

		else if (f=="20") cout << "2192834560";

		else if (f=="21") cout << "3099852800";

		else if (f=="22") cout << "3772252160";

		else if (f=="23") cout << "862453760";

		else if (f=="24") cout << "3519021056";

		else if (f=="25") cout << "2076180480";

		else if (f=="26") cout << "2441084928";

		else if (f=="27") cout << "1484783616";

		else if (f=="28") cout << "2919235584";

		else if (f=="29") cout << "3053453312";

		else if (f=="30") cout << "1409286144";

		else if (f=="31") cout << "738197504";

		else if (f=="32") cout << "2147483648";

		else if (f=="33") cout << "2147483648";
		
		else cout<<"0";
		
		cout<<endl;
	}

	return 0;
}