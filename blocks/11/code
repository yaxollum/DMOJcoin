#include <cmath>
#include <iostream>

using namespace std;

int factors(int num)
{
    int nf=0;
    int sn=sqrt(num);
    for(int f=1;f<=sn;++f)
    {
        if (num%f==0)
        {
            ++nf;
            if (f!=num/f) ++nf;
        }
    }
    return nf;
}

int main()
{
    int n;
    cin>>n;
    
    while(factors(n)!=2)
    {
        ++n;
    }
    cout<<n;
}