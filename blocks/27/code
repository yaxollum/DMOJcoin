#include <stdio.h>
#include <string.h>

char mp[]="pppppppppppppppppppppppppppppppp !Q#$%&q()*}w'e[0123456789ZzW]E{@ANIHDYUJGCVPMLSRXO:KF><BT?-\\=^\"`anihdyujgcvpmlsrxo;kf.,bt/_|+~";
char str[260];

int main() 
{
    fgets(str,256,stdin);
    int n=strlen(str)-1;
    str[n]=0;
    
    for(int i=0;i<n;++i)
    {
        str[i]=mp[str[i]];
    }
    puts(str);
    return 0;
}