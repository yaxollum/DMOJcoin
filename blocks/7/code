#include <iostream>

using namespace std;

int main()
{
	int n;
	cin >> n;

	int target;
	cin >> target;
	target = abs(target);

	int interval;
	int best_dist;
	bool possible = false;

	for (int loop = 0; loop < n; ++loop)
	{
		cin >> interval;

		if (target%interval == 0)
		{
			if (possible)
			{
				if ((target / interval) < best_dist) best_dist = target / interval;
			}
			else
			{
				possible = true;
				best_dist = target / interval;
			}
		}
	}

	if (possible) cout << best_dist << endl;
	else cout << "This is not the best time for a trip." << endl;
	return 0;
}